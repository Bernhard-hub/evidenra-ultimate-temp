{
  "name": "evidenra-ultimate",
  "version": "1.2.0",
  "description": "EVIDENRA Ultimate - AKI Method Research Tool",
  "main": "src/main/main.js",
  "scripts": {
    "start": "electron .",
    "dev": "webpack serve --mode development",
    "build": "webpack --mode development",
    "build-prod": "webpack --mode production",
    "electron-dev": "set NODE_ENV=development && electron .",
    "pack": "electron-builder --dir",
    "dist": "npm run build-prod && electron-builder",
    "dist:win": "npm run build-prod && electron-builder --win portable",
    "dist:mac": "npm run build-prod && electron-builder --mac",
    "dist:linux": "npm run build-prod && electron-builder --linux",
    "dist:all": "npm run build-prod && electron-builder --win portable --mac --linux",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css}\"",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage"
  },
  "keywords": [
    "qualitative research",
    "grounded theory",
    "EVIDENRA"
  ],
  "author": {
    "name": "Bernhard",
    "email": "evidenra@example.com"
  },
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-react": "^7.27.1",
    "@babel/preset-typescript": "^7.27.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/d3": "^7.4.3",
    "@types/react": "^19.1.12",
    "@types/react-dom": "^19.1.9",
    "@typescript-eslint/eslint-plugin": "^8.46.1",
    "@typescript-eslint/parser": "^8.46.1",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "app-builder-bin": "^5.0.0-alpha.12",
    "babel-loader": "^10.0.0",
    "electron": "^37.4.0",
    "electron-builder": "^26.0.12",
    "eslint": "^9.38.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.0",
    "happy-dom": "^20.0.7",
    "html-webpack-plugin": "^5.6.4",
    "jsdom": "^27.0.1",
    "prettier": "^3.6.2",
    "ts-loader": "^9.5.4",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4",
    "webpack": "^5.101.3",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.2"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.86.0",
    "@tabler/icons-react": "^3.34.1",
    "@tensorflow/tfjs": "^4.22.0",
    "autoprefixer": "^10.4.21",
    "buffer": "^6.0.3",
    "css-loader": "^7.1.2",
    "d3": "^7.9.0",
    "density-clustering": "^1.3.0",
    "framer-motion": "^12.23.24",
    "html2canvas": "^1.4.1",
    "jspdf": "^2.5.2",
    "ml-matrix": "^6.12.1",
    "ml-pca": "^4.1.1",
    "node-machine-id": "^1.1.12",
    "pdf-parse": "^1.1.1",
    "pdfjs-dist": "^5.4.149",
    "postcss": "^8.5.6",
    "postcss-loader": "^8.2.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "recharts": "^3.3.0",
    "style-loader": "^4.0.0",
    "tailwindcss": "^3.4.17",
    "ws": "^8.18.3",
    "zustand": "^5.0.9"
  },
  "build": {
    "appId": "com.evidenra.ultimate",
    "productName": "EVIDENRA Ultimate",
    "directories": {
      "output": "releases/2025-12-17"
    },
    "win": {
      "target": {
        "target": "portable",
        "arch": [
          "x64"
        ]
      },
      "icon": "Logo.ico",
      "verifyUpdateCodeSignature": false,
      "signAndEditExecutable": false
    },
    "portable": {
      "artifactName": "EVIDENRA-Ultimate-${version}-Windows.exe"
    },
    "mac": {
      "target": {
        "target": "dmg",
        "arch": [
          "x64",
          "arm64"
        ]
      },
      "category": "public.app-category.education",
      "icon": "Logo.png",
      "artifactName": "EVIDENRA-Ultimate-${version}-macOS-${arch}.dmg"
    },
    "dmg": {
      "artifactName": "EVIDENRA-Ultimate-${version}-macOS-${arch}.dmg"
    },
    "linux": {
      "target": [
        "AppImage"
      ],
      "category": "Education",
      "icon": "Logo.png",
      "artifactName": "EVIDENRA-Ultimate-${version}-Linux.AppImage"
    },
    "publish": null,
    "buildDependenciesFromSource": false,
    "nodeGypRebuild": false,
    "extraResources": [
      {
        "from": "Logo.ico",
        "to": "Logo.ico"
      }
    ],
    "files": [
      "dist/**/*",
      "src/main/**/*",
      "src/preload/**/*",
      "package.json",
      "!node_modules/@napi-rs/canvas-android-arm64/**/*",
      "!node_modules/@napi-rs/canvas-*/**/*"
    ],
    "extraMetadata": {
      "main": "src/main/main.js"
    },
    "asarUnpack": [
      "node_modules/pdfjs-dist/**/*",
      "src/main/**/*"
    ],
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    },
    "electronDownload": {
      "cache": ".electron-cache"
    }
  }
}
